<UserControl
    x:Class="KnowYourCeleb.Controls.Snapped"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:local="using:KnowYourCeleb.Controls"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    mc:Ignorable="d" Height="800" Width="320">
	<Grid Background="{StaticResource ApplicationPageBackgroundThemeBrush}" UseLayoutRounding="True">
		<Grid HorizontalAlignment="Left" Height="298" Margin="51,19,0,0" VerticalAlignment="Top" Width="210">
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="109*"/>
				<ColumnDefinition Width="102*"/>
			</Grid.ColumnDefinitions>
			<RadioButton Name="RadioRookie" Content="Nybörjare" IsChecked="True" HorizontalAlignment="Left" Margin="41,131,0,0" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
			<RadioButton Name="RadioExpert" Content="Expert" Margin="41,165,13,0" VerticalAlignment="Top" Grid.ColumnSpan="2"/>
			<Button x:ConnectionId='5' x:Name="StartButton"                   Content="Starta Spel" HorizontalAlignment="Center" Margin="41,59,46,0" VerticalAlignment="Top" Height="47" Width="123" Grid.ColumnSpan="2"/>
			<TextBlock FontSize="24" HorizontalAlignment="Center" Margin="13,15,10,0" TextWrapping="Wrap" VerticalAlignment="Top" RenderTransformOrigin="0.093,0.667" Height="32" Width="187" Grid.ColumnSpan="2">
				<Run Text="Känn din"/>
				<Run Text=" "/>
				<Run Text="kändis"/>
			</TextBlock>
		</Grid>
		<Grid Name="GameButtons" HorizontalAlignment="Left" Height="311" Margin="19,529,0,-40" VerticalAlignment="Top" Width="291">
			<Button x:ConnectionId='1' Width="200" Name="First"                     Content="Button" HorizontalAlignment="Left" Margin="44,73,0,0" VerticalAlignment="Top"/>
			<Button x:ConnectionId='2' Width="200" Name="Second"                      Content="Button" HorizontalAlignment="Left" Margin="44,122,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.383,0.382"/>
			<Button x:ConnectionId='3' Width="200" Name="Third"                     Content="Button" HorizontalAlignment="Left" Margin="44,170,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.921,1.197"/>
			<Button x:ConnectionId='4' Width="200" Name="Fourth"                      Content="Button" HorizontalAlignment="Left" Margin="44,221,0,0" VerticalAlignment="Top" RenderTransformOrigin="-0.292,1.553"/>
			<Rectangle Name="Rektangle" Fill="#FFF4F4F5" HorizontalAlignment="Right" Height="48" Margin="0,4,47,259" Stroke="Black" VerticalAlignment="Center" Width="200"/>
		</Grid>
		<Grid HorizontalAlignment="Left" Height="305" Margin="10,219,-84,0" VerticalAlignment="Top" Width="385">
			<Grid.ColumnDefinitions>
				<ColumnDefinition Width="58*"/>
				<ColumnDefinition Width="327*"/>
			</Grid.ColumnDefinitions>
			<Canvas x:Name="MainCanvas" HorizontalAlignment="Left" Height="250" Margin="34,32,0,0" VerticalAlignment="Top" Width="250" RenderTransformOrigin="-0.21,0.08" Background="White" Grid.ColumnSpan="2"/>
		</Grid>
	</Grid>
</UserControl>


